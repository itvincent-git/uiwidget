.grid-container{position:relative;overflow:hidden;border:1px solid #B7CAD1;clear:both;}
.grid-main-wrap{position:relative;}
.grid-mainhead-wrap, .grid-summary-bar {position: relative;z-index:2;overflow:hidden;text-align:left;width:100%;padding-left:0px;}
.grid-mainbody-wrap{position: relative;overflow: auto;text-align:left;width:100%;height:100%;}
.grid-mainhead-scroller, .grid-mainbody-scroller, .grid-summary-bar-scroller{float:left;width:10000px; position:relative;}
.grid-hd-table, .grid-bd-table, .grid-summary-table {margin-left:0px;table-layout: fixed;}
.grid-autosize .grid-bd-table, .grid-autosize .grid-hd-table, .grid-autosize .grid-summary-table{width:100%;}
.grid-hd-table{margin-right:100px}
.grid-hd-row td, .grid-bd-row td, .grid-summary-row td {font-size:12px;	height:24px;border-bottom:1px solid #B7CAD1;vertical-align: top;white-space:nowrap;word-break:keep-all;word-wrap:normal;text-overflow: ellipsis;-o-text-overflow: ellipsis;-moz-outline: none;-moz-user-focus: normal;outline:0;}
.grid-hd-row td,.grid-summary-row td {background-color:#F3F9F8;vertical-align: middle;}
.grid-row{border-right:1px solid #B7CAD1;width:100%}
.grid-inner{qdisplay: block;padding : 3px 3px 3px 5px;height:18px;line-height:18px;overflow: hidden;white-space:nowrap;word-break:keep-all;word-wrap:normal;text-overflow: ellipsis;-o-text-overflow: ellipsis;	position: relative;}
.grid-hd-row td .grid-inner {-moz-user-select: none;-khtml-user-select: none;}
.grid-hd-row-hidden td .grid-inner{display:none;padding-top: 0px; padding-bottom: 0px; height: 0px;}
.grid-hd-row-hidden td{height:0px;border-bottom:0px;}
.grid-hd-over{cursor:pointer;background:url(../images/bg-listtable-sort.gif) repeat-x top}
.grid-hd-tool {display: block;position: absolute;z-index: 2;height: 20px;top: 2px;right: 0px;}
.grid-hd-tool span {float:left;font-size:1px;}
.grid-hd-sort {position: relative;left:0px;top:0px;width:10px;height:5px;}
.grid-hd-asc{background:url(../images/sort_asc.gif) no-repeat left center}
.grid-hd-desc{background:url(../images/sort_desc.gif) no-repeat left center}
.grid-hd-split {qdisplay:block;height: 16px;width:2px;overflow: hidden;background:url(../images/img-grid-title-line.gif) no-repeat right center;}
.grid-empty-text{font-size:12px;height:15px;*height:14px;padding:5px;text-align:center;color:#777}
.grid-row{background-color:#fff}
.grid-row-over{ background-color:#F2F2F2}
.grid-row-selected{background-color:#FFF4B4}
.grid-cell-selected{background-color:#D7F0F7}
.grid-focus{position:absolute; left:-100px; top:-100px; width:1px; height:1px; line-height:1px; font-size:1px; -moz-outline:0 none; outline:0 none; -moz-user-select:text; -khtml-user-select:text}
.grid-cm-gridRowNumberer, .grid-cm-gridPageRowNumberer{text-align:center}

.grid-bottom-toolbar{height:26px; line-height:26px; border:0; padding:1px 7px 1px 7px; *padding:1px 0px 1px 7px; background-color:#F3F9F8; -moz-user-select:moz-none; border-top:1px solid #C3D6DD}
.grid-bottom-toolbar .display-text{float:left}
.grid-bottom-toolbar .page-size-list{height:26px; line-height:26px; border:0; padding:0px 15px 0px 0px; float:left; text-align:center}
.grid-bottom-toolbar .page-navigator{float:right; *margin-right:4px}
.grid-bottom-toolbar .page-number{color:#000; background-color:#fff; height:10px; border:1px solid #99B1BB; vertical-align:middle; height:16px; width:25px; text-align:right; padding-top:2px}
.page-next, .page-pre, .page-first, .page-last{cursor:pointer; white-space:nowrap; padding:0px; height:16px; width:16px; border:0px;     outline:none;       -moz-outline-style:none;     -moz-opacity:0.6; opacity:.6; filter:alpha(opacity = 60)}
.page-next-over, .page-pre-over, .page-first-over, .page-last-over{-moz-opacity:1; opacity:1; filter:alpha(opacity = 100)}
.grid-bottom-toolbar .page-next{background:url('../images/ico-page-next.gif') center no-repeat}
.grid-bottom-toolbar .page-next-disabled{background:url('../images/ico-page-next-disabled.gif') center no-repeat}
.grid-bottom-toolbar .page-pre{background:url('../images/ico-page-prev.gif') center no-repeat}
.grid-bottom-toolbar .page-pre-disabled{background:url('../images/ico-page-prev-disabled.gif') center no-repeat}
.grid-bottom-toolbar .page-first{background:url('../images/ico-page-first.gif') center no-repeat}
.grid-bottom-toolbar .page-first-disabled{background:url('../images/ico-page-first-disabled.gif') center no-repeat}
.grid-bottom-toolbar .page-last{background:url('../images/ico-page-last.gif') center no-repeat}
.grid-bottom-toolbar .page-last-disabled{background:url('../images/ico-page-last-disabled.gif') center no-repeat}
.grid-bottom-toolbar .page-seperator{color:#99B1BB}
.grid-btn-order{cursor:pointer}
.grid-btn-order ul, .grid-btn-order li{float:left}
.grid-btn-order .left-normal, .grid-btn-order .right-normal, .grid-btn-order .left-over, .grid-btn-order .right-over{    width:3px;     height:22px}
.grid-btn-order .middle-normal, .grid-btn-order .middle-over{height:22px; _height:18px; line-height:21px; text-align:center; padding-left:8px; padding-right:8px; _padding-top:1px; white-space:nowrap; display:block}
.grid-btn-order .middle-normal img, .grid-btn-order .middle-over img{    margin-right:5px}
.grid-btn-order .left-normal{background:url(../images/bg-btn-lr.gif) no-repeat 0 0}
.grid-btn-order .middle-normal{background:url(../images/bg-btn-middle.gif) repeat-x 0 0}
.grid-btn-order .right-normal{background:url(../images/bg-btn-lr.gif) no-repeat 0 -22px}
.grid-btn-order .left-over{background:url(../images/bg-btn-lr.gif) no-repeat 0 -44px}
.grid-btn-order .middle-over{    background:url(../images/bg-btn-middle.gif) repeat-x 0 -22px}
.grid-btn-order .right-over{    background:url(../images/bg-btn-lr.gif) no-repeat 0 -66px}


.grid-bottom-ctoolbar{width:100%;height:26px;padding:0 5px 0 5px; background-color:#f6f6f6; border-top:1px solid #C3D6DD;}
.grid-bottom-ctoolbar ul, .grid-bottom-ctoolbar li{padding:0; margin:0; list-style-type:none;font-size:12px;}
.grid-bottom-ctoolbar li{height:26px;line-height:26px; float:left; text-align:center}
.grid-bottom-ctoolbar .display-text{color:#666; margin-right:1px; text-align:left; }
.grid-bottom-ctoolbar .display-text .bold{font-weight:bold; margin:0 5px 0 5px}
.grid-bottom-ctoolbar .page-size-list{color:#666;margin-right:1px; text-align:left;_height:24px;_padding-top:2px;}
.grid-bottom-ctoolbar .page-size-list-select{font-size:12px;width:50px;vertical-align: middle;}
.grid-bottom-ctoolbar .display-line{margin-right:5px; background:url(../images/line.gif) no-repeat right center}
.grid-bottom-ctoolbar .page, .grid-bottom-ctoolbar .current{padding:0 7px 0 7px}
.grid-bottom-ctoolbar .ellipsis{letter-spacing:-2px}
.grid-bottom-ctoolbar .nav a{display:inline-block; text-decoration:none; outline:0;color:#666;float:left;}
.grid-bottom-ctoolbar .nav a:hover{text-decoration:underline; background-color:#d8d8d8}
.grid-bottom-ctoolbar .current{font-weight:bold; background-color:#fff4b4; display:block;float:left;}
.grid-bottom-ctoolbar .page-number{font-size:12px;width:26px; height:14px; color:#999; padding:2px; border:1px solid #868686; background-color:#fff; margin:3px 0 0 10px}


.grid-summary-bar{border-top:1px solid #C3D6DD;}
.grid-summary-bar .grid-summary-table td{font-weight:bold}
.grid-summary-bar .grid-summary-table td{border-bottom:0;}